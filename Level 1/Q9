import java.util.*;

public class Main{

public static void main(String[] args) {
  // write your code here  
  
  Scanner sc=new Scanner(System.in);
  int n=sc.nextInt();
  int x=1,inverse=0;
  while(n>0){
      int rem=n%10;
      inverse+=x*(int)Math.pow(10,rem-1);
      n/=10;
      x++;
      
      
  }
  
  
  
  System.out.print(inverse);
  
  
  
  
  
  
  
  
  
  
 }
}